{
  "name": "justoverclock/flarum-ext-visitorstat",
  "description": "Add a simple visitor counter to sidenav",
  "keywords": [
    "flarum"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "require": {
    "flarum/core": "^1.0.0"
  },
  "require-dev": {
    "flarum/testing": "^1.0.0"
  },
  "authors": [
    {
      "name": "Marco Colia",
      "email": "info@flarum.it",
      "role": "Developer",
        "homepage": "https://flarum.it"
    }
  ],
  "autoload": {
    "psr-4": {
      "Justoverclock\\VisitorStat\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Justoverclock\\VisitorStat\\Tests\\": "tests/"
    }
  },
  "extra": {
    "flarum-extension": {
      "title": "Visitorstat",
      "category": "",
      "icon": {
        "name": "fas fa-sort-numeric-up-alt",
        "backgroundColor": "blue",
        "color": "white"
      }
    },
    "flagrow": {
      "discuss": ""
    }
  }
}
